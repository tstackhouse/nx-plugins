{
  "$schema": "../../node_modules/@angular-devkit/architect/src/builders-schema.json",
  "builders": {
    "build": {
      "implementation": "./src/builders/build/build.impl",
      "schema": "./src/builders/build/schema.json",
      "description": "Build a Serverless application"
    },
    "compile": {
      "implementation": "./src/builders/compile/compile.impl",
      "schema": "./src/builders/compile/schema.json",
      "description": "Compiles a Serverless Typescripted application"
    },
    "offline": {
      "implementation": "./src/builders/offline/offline.impl",
      "schema": "./src/builders/offline/schema.json",
